{
  "name": "3d-floor-plan",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build-dev": "vite build --mode dev",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/highlight": "^3.12.0",
    "@react-pdf-viewer/search": "^3.12.0",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.6.0",
    "axios": "^1.5.1",
    "jwt-decode": "^4.0.0",
    "lottie-web": "^5.12.2",
    "moment": "^2.30.1",
    "pdfjs-dist": "^3.4.120",
    "qs": "^6.12.0",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-lazylog": "^4.5.3",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-virtualized": "^9.22.5",
    "sass": "^1.54.3",
    "simplebar-react": "^2.4.3",
    "styled-components": "^5.3.9",
    "uuid": "^9.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@ljharb/eslint-config": "^21.1.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-react": "^7.30.1",
    "vite": "^3.0.0",
    "vite-plugin-eslint": "^1.7.0"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "always",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": true,
    "endOfLine": "lf"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
    ],
    "rules": {
      "max-len": [
        "error",
        120
      ],
      "max-lines": [
        "error",
        200
      ],
      "camelcase": [
        "error",
        {
          "properties": "never",
          "ignoreDestructuring": true
        }
      ],
      "max-params": [
        "error",
        2
      ],
      "no-alert": 2,
      "no-console": 2,
      "no-debugger": 1,
      "no-duplicate-imports": 1,
      "no-var": 2,
      "quotes": [
        2,
        "single",
        {
          "avoidEscape": true
        }
      ],
      "react/display-name": 0,
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "semi": [
        "error",
        "always"
      ],
      "strict": 2
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2015,
      "requireConfigFile": false,
      "allowImportExportEverywhere": true,
      "babelOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
